load("render.star", "render")
load("encoding/base64.star", "base64")
load("http.star", "http")
load("random.star", "random")

IMG_DATA = [

""

,

""

,

""

]

# data for collection items and images
BrandNames = ["Armin Strom", "Christopher Ward", "Casio"]
ModelNames = ["Gravity Water", "Morgan Aero 8", "MRW-200H"]
Years = ["2013", "2020", "2018"]
Widths = [21, 21, 21]
Heights = [32, 33, 33]

# set colors
white_color="#FFFFFF" # white

def main():

    i = random.number(0, 2)
    #print(i)
    BRAND_NAME = BrandNames[i]
    MODEL_NAME = ModelNames[i]
    IMG_WIDTH = Widths[i]
    IMG_HEIGHT = Heights[i]
    IMAGE = base64.decode(IMG_DATA[i])
	
    return render.Root(
        child = render.Column(
          children=[
            render.Row(
                main_align="start",	# horizontal alignment
                cross_align="center",	# vertical alignment
                expanded=True,
                children = [
                    render.Image(src=IMAGE, width=IMG_WIDTH, height=IMG_HEIGHT),
                    render.Column(
                      main_align="center",
                      cross_align="start",
                      expanded=True,
                      children=[
                        render.Row(
			main_align="start",
			cross_align="center",
			expanded=True,
			children = [
				render.WrappedText(BRAND_NAME, color=white_color, font="tom-thumb"),
			],
                        ),
                        render.Row(
			main_align="end",
			cross_align="center",
			expanded=True,
			children = [
				render.WrappedText(MODEL_NAME, color=white_color, font="tom-thumb"),
			],
                        ),
                      ],
                    ),
                ],
            ),

          ],
        ),
    )